R"##(

(define (standard list)
  (environment ()

    (define null?
      (lambda (x)
        (eq? x '())))

    (define list
      (lambda x x))

    ; TODO list?

    (define list-tail
      (lambda (x k)
        (if (zero? k) x
            (list-tail (cdr x) (- k 1)))))

    (define list-ref
      (lambda (x k)
        (car (list-tail x k))))
    )
  )

)##"

